                <td>{{ s_a_v.endDate ? s_a_v.endDate|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>RepairedBy</th>
                <td>{{ s_a_v.repairedBy }}</td>
            </tr>
            <tr>
                <th>Repairs</th>
                <td>{{ s_a_v.repairs }}</td>
            </tr>
            <tr>
                <th>Comments</th>
                <td>{{ s_a_v.comments }}</td>
            </tr>
            <tr>
