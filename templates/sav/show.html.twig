    <h1>SAV</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ s_a_v.id }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ s_a_v.code }}</td>
            </tr>
            <tr>
